package Day5;
import java.util.Scanner;
public class Palindrome {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		int rev=0;
		int temp=num;
		while(num>0) {
			 temp=num/10;
			rev=rev*10+temp;
			 num= num/10;	
	    }
		if(temp==num) {
			System.out.println("It is palindrome");
		}
		else {
			System.out.println("It is not a palindrome");
		}
		}

	}


